{
  "*.{js,jsx}": [
    "prettier --write",
    "eslint --fix --quiet --config .eslintrc.json --resolve-plugins-relative-to .",
    "git add"
  ],
  "*.{ts,tsx}": [
    "prettier --write",
    "eslint --fix --quiet --config .eslintrc.json --resolve-plugins-relative-to .",
    "git add"
  ],
  "*.{scss,scss,css,md,html}": ["prettier --write", "git add"]
}
